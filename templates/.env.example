# Halldyll Environment Variables
# Copy this file to .env and fill in your values

# Required: RunPod API Key
# Get it from: https://www.runpod.io/console/user/settings
RUNPOD_API_KEY=

# Optional: HuggingFace Token (for gated models)
# Get it from: https://huggingface.co/settings/tokens
HF_TOKEN=

# Optional: AWS credentials for S3 state backend
# AWS_ACCESS_KEY_ID=
# AWS_SECRET_ACCESS_KEY=
# AWS_REGION=us-east-1

# Optional: Override configuration values
# HALLDYLL_PROJECT_NAME=my-project
# HALLDYLL_PROJECT_ENVIRONMENT=prod
